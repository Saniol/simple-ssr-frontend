{
    "extends": [
        "airbnb",
        "airbnb/hooks",
    ],
    "globals": {
        "window": false,
        "document": false,
    },
    "parser": "babel-eslint",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
    }
}